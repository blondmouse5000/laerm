{
  "name": "laerm",
  "version": "1.1.2",
  "description": "lauter laerm website",
  "private": true,
  "scripts": {
    "clean": "rm -f web/style.css*",
    "test:html": "html-validate web/index.html",
    "test": "npm run test:html",
    "build:css": "sass style/style.scss web/style.css --style compressed",
    "build:js": "haxe build.hxml",
    "build": "npm run build:css && npm run build:js",
    "dev": "sass style/style.scss web/style.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/disktree/laerm.git"
  },
  "author": "tong@disktree.net",
  "license": "ISC",
  "homepage": "https://github.com/disktree/laerm/blob/master/README.md",
  "devDependencies": {
    "sass": "^1.32.6",
    "html-validate": "^4.5.0"
  }
}
